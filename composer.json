{
    "name": "simple-bus/rabbitmq-bundle",
    "type": "library",
    "description": "Use RabbitMQ with SimpleBus/Asynchronous",
    "keywords": ["RabbitMQ", "messages", "message bus", "event bus", "command bus"],
    "homepage": "http://github.com/SimpleBus/RabbitMQBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "beberlei/assert": "~2.0",
        "simple-bus/message-bus": "~1.0",
        "simple-bus/asynchronous-bundle": "dev-master",
        "simple-bus/asynchronous": "dev-master",
        "oldsound/rabbitmq-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "simple-bus/jms-serializer-bundle": "dev-master",
        "symfony/console": "~2.0",
        "symfony/finder": "~2.0",
        "symfony/process": "~3.0@dev",
        "matthiasnoback/phpunit-asynchronicity": "dev-master"
    },
    "autoload": {
        "psr-4" : { "SimpleBus\\RabbitMQBundle\\" : "src" }
    },
    "autoload-dev": {
        "psr-4": { "SimpleBus\\RabbitMQBundle\\Tests\\": "tests" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
